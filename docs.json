{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Keylo Documentation",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.keyloroblox.xyz",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Main Site",
          "href": "https://keyloroblox.xyz",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/Keylo_White.png",
    "dark": "/logo/Keylo_White.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@keyloroblox.xyz"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://keyloroblox.xyz/workspaces"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/HXj8m4wM",
      "youtube": "https://www.youtube.com/@KeyloRoblox_Offical",
      "github": "https://github.com/KeyloGroup"
    }
  }
}
